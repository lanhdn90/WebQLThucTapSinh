
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AdminMaster</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/sb-admin-2.css" rel="stylesheet" />
    <link href="~/Content/metisMenu.css" rel="stylesheet" />
    <link href="~/Content/fontawesome-free-5.15.1-web/css/all.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/dataTables.bootstrap.css" rel="stylesheet" />
</head>
<body>
    <div id="wrapper">
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">

            <div class="navbar-header">
                <img class="logo" style="height: 50px; padding-left: 20px; width: 250px;" src="~/Images/logo.png">
            </div>
            <ul class="nav navbar-top-links navbar-right">
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-user fa-fw"></i> @*@Session["TenTk"]*@ <i class="fa fa-caret-down"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li>
                            <a href="/Admin/DangXuat"><i class="fas fa-address-card fa-fw"></i> Thông tin Cá nhân</a>
                        </li>
                        <li>
                            <a href="/Admin/DangXuat"><i class="fas fa-sign-out-alt fa-fw"></i> Đăng xuất</a>
                        </li>
                    </ul>
                    <!-- /.dropdown-user -->
                </li>
                <!-- /.dropdown -->
            </ul>
            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li>
                            <a href="#"><i class="fa fa-home fa-fw"></i> Trang chủ</a>
                        </li>
                        <li>
                            <a href="/School/Index/"><i class="fas fa-school fa-fw"></i> Quản lý Nhà trường</a>
                        </li>
                        <li>
                            <a href="/Company/Index/"><i class="fas fa-building fa-fw"></i> Quản lý Công ty</a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-link fa-fw"></i> Báo cáo thống kê</a>
                        </li>
                        <li>
                            <a href="/Company/Edit/"><i class="fas fa-building fa-fw"></i> Thông tin Công ty</a>
                        </li>
                        <li>
                            <a href="/Leader/Index"><i class="fas fa-address-card fa-fw"></i> Hướng dẫn viên</a>
                        </li>
                        <li>
                            <a href="/Faculty/Index"><i class="fas fa-university fa-fw"></i> Quản lý Khoa</a>
                        </li>
                        <li>
                            <a href="/Representative/Index"><i class="fas fa-address-card fa-fw"></i> Giáo vụ</a>
                        </li>
                        <li>
                            <a href="/InternShip/Index/0"><i class="fa fa-clipboard fa-fw"></i> Quản lý Khóa học</a>
                        </li>
                        <li>
                            <a href="/Tasks/Index"><i class="fa fa-book fa-fw"></i> Quản lý Bài học</a>
                        </li>
                        <li>
                            <a href="/Question/Index"><i class="fas fa-question-circle fa-fw"></i> Quản lý Câu hỏi</a>
                        </li>
                        <li>
                            <a href="/School/Edit/"><i class="fas fa-school fa-fw"></i> Thông tin Nhà trường</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-graduation-cap fa-fw"></i> Quản lý Thực tập sinh</a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-book-reader fa-fw"></i> Khóa học</a>
                        </li>
                        <li>
                            <a href="#"><i class="fas fa-tasks fa-fw"></i> Kết quả Kiểm tra</a>
                        </li>
                    </ul>
                </div>

            </div>
            <!-- /.navbar-top-links -->
        </nav>
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>

    </div>
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/metisMenu.js"></script>
    <script src="~/Scripts/sb-admin-2.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    <script src="~/Content/ckfinder/ckfinder.js"></script>
    <script src="~/Content/ckeditor/ckeditor.js"></script>
    @RenderSection("jsFooter", required: false)
    <script>
        $(document).ready(function () {
            $('#table').DataTable({
                responsive: true
            });
        });
    </script>

</body>
</html>

